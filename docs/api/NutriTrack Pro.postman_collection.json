	"item": [
		{
			"name": "Auth",
			"item": [
				{
					"name": "Login Profesional",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Token is present and set as collection variable\", function () {",
									"    var jsonData = pm.response.json();",
									"    if (jsonData.token) {",
									"        pm.collectionVariables.set(\"authToken\", jsonData.token);",
									"        console.log(\"Auth token set as collection variable.\");",
									"    } else {",
									"        console.log(\"Token not found in response.\");",
									"    }",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"email\": \"tu_email_de_profesional@example.com\",\n  \"password\": \"tu_contraseña_de_profesional\"\n}"
						},
						"url": {
							"raw": "{{baseUrl}}/auth/login",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"auth",
								"login"
							]
						},
						"description": "Autentica a un profesional y guarda el token JWT en la variable de colección `authToken`."
					},
					"response": []
				}
			],
			"description": "Endpoints relacionados con la autenticación"
		},
